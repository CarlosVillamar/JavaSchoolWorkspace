	  		System.out.println("Enter the haccking sequence you would like to try: ");
	  		long hackSeqLength = scanner.nextLong();
	  		char[] programSeq = scanner.next().toCharArray();
	  	}
	   System.exit(0);
	  	
	  	
  }
	
}
