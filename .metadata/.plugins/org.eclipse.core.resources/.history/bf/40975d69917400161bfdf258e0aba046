package Recursion;

public class arrSwap
{
	public static void main()
	{	int[] arr = new int[20];
		swap(arr,2,4);
	}
		
	public static void swap(int[] arr,int i, int j)
	{	
			int z =arr[i];
			arr[i]=arr[j];
			arr[j]=z;
			for(int x =0; x<arr.length;x++)
			{
				System.out.println(x + arr[x] + "");
			
			}
			
			
	}
	
	 
}
